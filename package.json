      "private": true
    }]
  },
  "repository": "https://github.com/ooAKLoo/electron-update.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^25.1.0",
    "electron-builder": "^22.13.1",
    "electron-updater": "^4.5.1"
  }
}
